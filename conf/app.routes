# microservice specific routes

GET        /                                     @controllers.CustomerDetailsController.show

GET        /change-business-address              @controllers.BusinessAddressController.initialiseJourney

GET        /hello-world                          @controllers.HelloWorldController.helloWorld

GET        /change-business-name                 @controllers.ChangeBusinessNameController.show

GET        /change-business-address/confirmation @controllers.BusinessAddressController.callback(id: String)

GET        /text-input                           @controllers.HelloWorldController.textInput
GET        /date-input                           @controllers.HelloWorldController.dateInput

GET        /frequency               @controllers.returnFrequency.ChooseDatesController.show
